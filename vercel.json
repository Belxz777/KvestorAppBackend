{
    "version":2,
    "name":"backendkvest",
    "env":{
        "POSTGRES_PORT": 5433,
       " POSTGRES_HOST" : "localhost",
        "POSTGRES_USER":"postgres",
        "POSTGRES_DB"  :"postgres",
        "POSTGRES_PASSWORD":123
        
    },
    "builds":[
        {
            "src":"src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
        "src":"/(.*)",
        "dest": "src/main.ts",
        "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE" ,
        "PATCH"
        ]
    }
]
}