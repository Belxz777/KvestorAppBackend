{
    "version": 2,
    "name": "backend-kvest",
    "builds": [
      {
        "src": "dist/main.js",
        "use": "@vercel/node"
      }
    ],
    "env": {
      "POSTGRES_PORT": "5433",
      "POSTGRES_HOST": "localhost",
      "POSTGRES_USER": "postgres",
      "POSTGRES_DB": "postgres",
      "POSTGRES_PASSWORD": "123"
    },
    "routes": [
      {
          "src": "/(.*)",
          "dest": "dist/main.js"
      }
  ]
  }